    <li class="email">
      <a data-email-name="josh" data-email-domain="forisha.com">
        [email&nbsp;protected]
      </a>
    </li>
  </ul>

  <script>
    (function () {
      var emailAddress;
      var emailLink;
      var emailLinks = document.querySelectorAll('a[data-email-name][data-email-domain]');

      for (var i = 0; i < emailLinks.length; i++) {
        emailLink = emailLinks[i];
        emailAddress = emailLink.getAttribute('data-email-name') +
          '@' + emailLink.getAttribute('data-email-domain');

        emailLink.setAttribute('href', 'mailto:' + emailAddress);
        emailLink.textContent = emailAddress;
      }
    }());
  </script>
